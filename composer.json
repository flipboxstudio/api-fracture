{
    "name": "flipboxstudio/api-fracture",
    "description": "Fracture is a library to make your Laravel-based API response more consistent",
    "require": {
        "php": ">=7.0.0",
        "league/fractal": "^0.13.0",
        "laravel/framework": "5.3.*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.5"
    },
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Flipbox\\Fracture\\": "src/Flipbox/Fracture/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/Test/"
        },
        "classmap": [
            "tests/TestCase.php"
        ]
    },
    "authors": [
        {
            "name": "Krisan Alfa Timur",
            "email": "krisan47@gmail.com"
        }
    ]
}
